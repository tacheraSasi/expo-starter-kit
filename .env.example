# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:8080
EXPO_PUBLIC_API_VERSION=v1

# Environment (development, staging, production)
EXPO_PUBLIC_ENVIRONMENT=development

# Feature Flags
EXPO_PUBLIC_ENABLE_ANALYTICS=false
EXPO_PUBLIC_ENABLE_CRASH_REPORTING=false

# Debug
EXPO_PUBLIC_DEBUG=true

# Auth Configuration
EXPO_PUBLIC_AUTH_DOMAIN=your-auth-domain

# Storage Configuration
EXPO_PUBLIC_STORAGE_BUCKET=your-storage-bucket-name

# Social Media Configuration (optional)
# EXPO_PUBLIC_GOOGLE_CLIENT_ID=your-google-client-id
# EXPO_PUBLIC_FACEBOOK_APP_ID=your-facebook-app-id

# Analytics (optional)
# EXPO_PUBLIC_GOOGLE_ANALYTICS_ID=your-ga-id
# EXPO_PUBLIC_MIXPANEL_TOKEN=your-mixpanel-token